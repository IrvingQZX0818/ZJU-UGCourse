<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="root" prefHeight="136.0" prefWidth="147.0" xmlns="http://javafx.com/javafx/8.0."
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.RegisterFormController">
    <VBox layoutX="188.0" layoutY="152.0" prefHeight="402.0" prefWidth="439.0" AnchorPane.bottomAnchor="0.0"
          AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <TextField fx:id="nickName" promptText="Nick name"/>
        <TextField fx:id="mail" promptText="E-mail address"/>
        <PasswordField fx:id="password" promptText="Password"/>
        <PasswordField fx:id="repeatPassword" promptText="Confirm password"/>
        <HBox prefHeight="100.0" prefWidth="200.0">
            <Button fx:id="registerButton" mnemonicParsing="false" text="Register"/>
            <Button fx:id="cancelButton" mnemonicParsing="false" text="Cancel"/>
        </HBox>
    </VBox>
</AnchorPane>
